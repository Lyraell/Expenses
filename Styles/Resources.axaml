<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Color x:Key="PrimaryColor">#ff34854d</Color>
	<Color x:Key="SecondaryColor">#ffc2db65</Color>
	<Color x:Key="HeaderTextColor">#ff9e7564</Color>
	<Color x:Key="BackgroundColor">#ff4f6a00</Color>

	<SolidColorBrush x:Key="HeaderTextBrush" Color="{StaticResource HeaderTextColor}" />
	<SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}" />
	<SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}" />
	<SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}" />

	<FontFamily x:Key="DefaultFont">avares://Expenses/Assets/Combo-Regular.ttf#Combo</FontFamily>
</ResourceDictionary>
